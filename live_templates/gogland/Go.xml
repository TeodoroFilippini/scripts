<templateSet group="Go">
  <template name="iferr" toReformat="false" toShortenFQNames="true" value="if err != nil {&#10;  panic(err.Error())&#10;}">
    <context>
      <option name="GO" value="true" />
    </context>
  </template>
  <template name="ifnotexists" toReformat="false" toShortenFQNames="true" value="if _, err := os.Stat($path$); os.IsNotExist(err) {&#10;  $end$&#10;}">
    <variable alwaysStopAt="true" defaultValue="" expression="" name="path" />
    <variable alwaysStopAt="true" defaultValue="" expression="" name="end" />
    <context>
      <option name="GO" value="true" />
    </context>
  </template>
</templateSet>
